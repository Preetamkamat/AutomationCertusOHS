<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CertusOHS Application Test" verbose="1" parallel="tests"
       thread-count="1" configfailurepolicy="continue">

    <listeners>
        <listener class-name="com.certusohs.extentreportlistener.ExtentReportListener" />
    </listeners>

    <test name="CertusOHS" annotations="JDK"
          preserve-order="true">
        <classes>
            <class name="com.certusohs.runner.TestRunner" />
        </classes>
    </test>
</suite>